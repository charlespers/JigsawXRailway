{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "buildCommand": "echo 'Using custom Dockerfile in backend/'",
    "dockerfilePath": "backend/Dockerfile",
    "watchPatterns": [
      "backend/**"
    ]
  },
  "deploy": {
    "startCommand": null,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}

